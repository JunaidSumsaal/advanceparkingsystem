{
  "files": [],
  "references": [
    {
      "path": "./tsconfig.app.json"
    },
    {
      "path": "./tsconfig.node.json"
    }
  ],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "src/*"
      ],
      "@api": [
        "./src/api/*"
      ],
      "@assets": [
        "./src/assets/*"
      ],
      "@components": [
        "./src/components/*"
      ],
      "@context": [
        "./src/context/*"
      ],
      "@pages": [
        "./src/pages/*"
      ],
      "@routes": [
        "./src/routes/*"
      ],
      "@services": [
        "./src/services/*"
      ],
      "@types": [
        "./src/types/*"
      ],
      "@utils": [
        "./src/utils/*"
      ]
    },
    "module": "ESNext",
    "moduleResolution": "Node",
    "target": "ESNext",
    "types": [
      "node"
    ],
    "esModuleInterop": true,
    "strict": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  }
}